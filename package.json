{
	"name": "@gisatcz/ptr-state",
	"version": "2.0.3",
	"description": "Panther FE state package",
	"keywords": [
		"panther",
		"gisat"
	],
	"homepage": "https://github.com/gisat-panther/ptr-state",
	"prettier": "@gisatcz/prettier-config",
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"files": [
		"dist",
		"lib"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/gisat-panther/ptr-state"
	},
	"peerDependencies": {
		"react": "^16.13.1 || ^17.0.2",
		"react-dom": "^16.13.1 || ^17.0.2"
	},
	"dependencies": {
		"@gisatcz/ptr-core": "^1.6.0",
		"@gisatcz/ptr-tile-grid": "^0.1.1",
		"@gisatcz/ptr-utils": "^1.5.4",
		"@jvitela/recompute": "^0.3.4",
		"@manaflair/redux-batch": "^1.0.0",
		"fast-stringify": "^2.0.0",
		"isomorphic-fetch": "^3.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"query-string": "^6.14.0",
		"re-reselect": "^4.0.0",
		"react-redux": "^7.2.6",
		"redux": "^4.1.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.4.0",
		"reselect": "^4.1.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/plugin-transform-runtime": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-react": "^7.16.0",
		"@babel/register": "^7.16.0",
		"@gisatcz/prettier-config": "^0.0.3",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^21.0.1",
		"babel-loader": "^8.2.3",
		"babel-plugin-istanbul": "^6.1.1",
		"babel-plugin-lodash": "^3.3.4",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"flat": "^5.0.2",
		"glob": "^7.2.0",
		"husky": "^7.0.4",
		"mocha": "^9.1.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"path": "^0.12.7",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.59.0",
		"rollup-plugin-filesize": "^9.1.0",
		"slash": "3.0.0"
	},
	"scripts": {
		"clean": "rimraf dist",
		"start": "run-s clean && run-p start:es start:cjs start:lib:es",
		"start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
		"start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
		"start:es": "cross-env NODE_ENV=es rollup -c -w",
		"build": "run-s clean && run-p build:es build:cjs build:lib:es",
		"build:es": "cross-env NODE_ENV=es rollup -c",
		"build:cjs": "cross-env NODE_ENV=cjs rollup -c",
		"build:lib:es": "npm-run-all build:babel:lib:es",
		"build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
		"test": "cross-env TZ=utc mocha",
		"test:watch": "cross-env TZ=utc mocha --watch",
		"coverage": "cross-env NODE_ENV=test TZ=utc nyc --reporter=html --reporter=json npm run test",
		"format": "prettier --write ."
	}
}
