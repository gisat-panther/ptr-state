{
  "name": "@gisatcz/ptr-state",
  "version": "1.5.7",
  "description": "Panther FE state",
  "keywords": [
    "panther",
    "gisat"
  ],
  "homepage": "https://github.com/gisat-panther/ptr-state",
  "style": "dist/style.css",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "lib"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisat-panther/ptr-state"
  },
  "dependencies": {
    "@gisatcz/ptr-core": "1.3.8",
    "@gisatcz/ptr-tile-grid": "0.0.2-alpha.5",
    "@gisatcz/ptr-utils": "1.3.7",
    "@jvitela/recompute": "0.3.3",
    "@manaflair/redux-batch": "1.0.0",
    "@taskworld.com/rereselect": "0.3.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "moment": "2.28.0",
    "path": "0.12.7",
    "query-string": "6.11.0",
    "re-reselect": "4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "6.0.1",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-multi-entry": "3.0.1",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "auto": "^9.43.1",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "glob": "^7.1.6",
    "mocha": "7.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "nyc": "^15.1.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "2.1.2",
    "rollup": "2.37.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "sass-loader": "10.1.1",
    "slash": "3.0.0",
    "source-map-loader": "2.0.0",
    "style-loader": "2.0.0",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.11"
  },
  "watch": {
    "test": {
      "patterns": [
        "src",
        "tests"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "run-s clean && run-p start:es start:cjs start:lib:es",
    "start:lib:es": "cross-env BABEL_ENV=es babel --watch src -d lib --verbose",
    "start:cjs": "cross-env NODE_ENV=cjs rollup -c -w",
    "start:es": "cross-env NODE_ENV=es rollup -c -w",
    "build": "run-s clean && run-p build:es build:cjs build:lib:es",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:lib:es": "npm-run-all build:babel:lib:es copy:stylesheets copy:assets",
    "build:babel:lib:es": "cross-env BABEL_ENV=es babel src -d lib",
    "copy:stylesheets": "node ./build/copy-stylesheets.js",
    "copy:assets": "node ./build/copy-assets.js",
    "test": "cross-env NODE_ENV=cjs rollup -c ./rollup.test.config.js && cross-env TZ=utc mocha -- --require source-map-support/register",
    "test:watch": "npm-watch test",
    "coverage": "cross-env NODE_ENV=cjs rollup -c ./rollup.test.config.js && cross-env TZ=utc nyc --reporter=html --reporter=text mocha -- --require source-map-support/register"
  }
}
